int printf(char* fmt, int d);

int main() {
	int x = 6;
	int y = 10;

	printf("x = %d\n", x);
	printf("y = %d\n", y);

	int a = !x;
	int b = ~x;
	int c = -x;
	int d = +x;

	printf("!x = %d\n", a);
	printf("~x = %d\n", b);
	printf("-x = %d\n", c);
	printf("+x = %d\n", d);

	int e = x < y;
	int f = x <= y;
	int g = x == y;
	int h = x >= y;
	int i = x > y;
	int j = x != y;
	int k = x && y;
	int l = x || y;

	printf("x < y = %d\n", e);
	printf("x <= y = %d\n", f);
	printf("x == y = %d\n", g);
	printf("x >= y = %d\n", h);
	printf("x > y = %d\n", i);
	printf("x != y = %d\n", j);
	printf("x && y = %d\n", k);
	printf("x || y = %d\n", l);

	int m = x + y;
	int n = x - y;
	int o = x * y;
	int p = x / y;
	int q = x % y;

	printf("x + y = %d\n", m);
	printf("x - y = %d\n", n);
	printf("x * y = %d\n", o);
	printf("x / y = %d\n", p);
	printf("x %% y = %d\n", q);

	int r = x & y;
	int s = x | y;
	int t = x >> y;
	int u = x << y;

	printf("x & y = %d\n", r);
	printf("x | y = %d\n", s);
	printf("x >> y = %d\n", t);
	printf("x << y = %d\n", u);

	return 0;
}
